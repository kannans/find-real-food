<%  unless @productsall.blank? %>
	$('#allbrands .products').append("<%=escape_javascript(render :partial => 'products/product', :collection => @productsall, :as => :product)%>");
	$('#nearby .products').append("<%=escape_javascript(render :partial => 'products/product', :collection => @products, :as => :product)%>");
	$('.view_all_products').attr('data-page','<%= @productsall.next_page %>')
<% else %>
	$('.view_all_products').attr('data-page','')
<% end %>

	   