<script type="text/javascript">
 
$(document).ready(function() {


      var next_page ='';
    var allpages = new Array(); 
    function lastAddedLiveFuncSearch()
    {
        
        if($('.view_all_products').attr('data-page')){
        
         next_page = $('.view_all_products').attr('data-page')+'a';
         
            if(key_exists(next_page, allpages)){
                     
            }else{
                
                allpages.push(next_page);
           
                
                
                var sort = '';
                var rank = '';
                var sold = '';
                <% if @category %>
                var category = <%= @category.id %>
                <% else %>
                var category = ''; 
                <% end  %>
                
                 if($('#proximity').is(":checked")){
                  sort = $('#proximity').val();
                 }else if($('#alphabetical').is(":checked")){
                   sort = $('#alphabetical').val();
                 }else if($('#rating').is(":checked")){
                     sort = $('#rating').val();
                 }

                 if($('#best').is(":checked")){
                  
                     rank = $('#best').val();
                 }else if($('#good').is(":checked")){
                  
                     rank = $('#good').val();
                 }else if($('#all').is(":checked")){
                  
                     rank = $('#all').val();
                 }
                 
                 
                 if($('#phone').is(":checked")){
                     sold = $('#phone').val();
                 }else if($('#online').is(":checked")){
                     sold = $('#online').val();
                 }else if($('#store').is(":checked")){
                     sold = $('#store').val();
                 }else if($('#all').is(":checked")){
                     sold = $('#all').val();
                 }

                  $.get("/searches/showmore", { page: $('.view_all_products').attr('data-page'),search: $("#search_field").val(),sort:sort,category:category, rank:rank, sold:sold }, null, "script");
                  return false;

          
            }
          
        }else{
            $('.view_all_products').hide();
        }
         return false;
    };
    
     function key_exists(key, search) {
    if (!search || (search.constructor !== Array && search.constructor !== Object)) {
        return false;
    }
    for (var i = 0; i < search.length; i++) {
        if (search[i] === key) {
            return true;
        }
    }
    return key in search;
    }

    $('.search_more').click(function(){
      
        lastAddedLiveFuncSearch();
    })

      
 
});
    

</script>
