                <div class="loading_ovelay">
                    <img src="/assets/product_loading.gif" alt="Loading..."/>
                </div>
                <div class="container">
                    <ul class='etabs'>
                        <li class='tab'><a href="#products" class="active">products</a></li>  
                       <li class='tab'><a href="#brands">brands</a></li>  
                      
                    </ul>
                    <div class="clear"></div>
                    <div class='tab_container fl'>
                        <div class='panel_container'>
                            <div id="products" class="tab_div active">
                                <div class="home_body prd_search">
                                    
                                    <div class="clear"></div>
                                    <div class="home_left fl">
                                        <% if @products %>
                                        <div class="products">
                                             
                                             <% @products.each do |product| %> 
                                             
                                            <%= render '/products/product_list' %>

                                            <% end %>

                                             
                                            <div class="expand">
                                                <a href="javascript:;"></a>
                                            </div>
                                            
                                        </div>
                                         <% else %>
                                            <div class="products"><p class="notification" >No Products Available</p>
                                            </div>

                                            

                                        <% end %>
                                    </div>
                                     
                                
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div id="brands" class="tab_div">
                               <div class="home_body prd_search">
                                    
                                    <div class="clear"></div>
                                    <div class="home_left fl">
                                        <% if @brands  %>
                                        <div class="products">
                                             
                                            <% @brands.each do |brand| %> 
                                           
                                            <div class="product_item">
                                                <div class="product_item_body">
                                                    <div class="media product">
                                                        <a href="javascript:;" class="media_img">
                                                         <% if brand.picture.present? %>
                                                        <%= image_tag (brand.picture(:thumb)) %>
                                                        <% else %>
                                                        <%= image_tag("product_no_image.png", :alt => brand.name) %>
                                                        <% end %>

                                                       </a>
                                                        <div class="media_text">
                                                            <p class="media_sub_title">
                                                                <% if brand.slug !='' %><a href="/brand/<%= brand.slug %>">
                                                                <% else %>
                                                                <a href="javascript:void(0);">
                                                                <% end %>
                                                                <%= brand.name%></a></p>
                                                      
                                                            <p class="location"><a href="javascript:void(0);">3 Location(s) Nearby</a></p>

                                                        </div>
                                                         <div class="clear"></div>
                                                           

                                                    </div>
                                                </div>
                                            </div>
                                            <% end %>
                                             
                                            <div class="expand">
                                                <a href="javascript:;"></a>
                                            </div>
                                           
                                        </div>
                                         <% else %>
                                            <div class="products"><p class="notification" >No Brands Available</p>
                                            </div>

                                            

                                        <% end %>
                                    </div>
                                    <div class="clear"></div>
                                </div>
                             </div>
                              
                        </div>
                    </div>

                    <div class="map_button fr hide_for_mobile with_tab">
                                        <a href="javascript:;">Full Screen Map</a>
                                    </div>
                                    <div class="map_button fr hide_for_desktop">
                                            <a href="javascript:;">Full Screen Map</a>
                                        </div>
                                        
                             <div class="home_right modified fr">
                                        <div class="home_right_container">
                                            <div class="map_area">
                                                <%= render '/layouts/zipsearch' %>
                                                <div id="map-canvas"></div>
                                            </div>
                                        </div>
                                    </div>

                    <div class="clear"></div>
                    <div class="stock_status">
                        <p><strong>IN STOCK?</strong> Please call the store to verify the product or brand you want is currently in stock before going.<br/>Find Real Food cannot guarantee product inventory on any given day.</p>
                    </div>
                </div>

                   